{
  "name": "raivokinne/minamell",
  "description": "simple php framework",
  "license": "MIT",
  "version": "1.0.0",
  "require": {
    "minamell/minamell": "dev-main",
    "vlucas/phpdotenv": "^5.6"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Test\\": "tests/"
    }
  },
  "config": {
    "process-timeout": 99999999999,
    "optimize-autoloader": true,
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
